<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GetReferenceFromDatabase">
    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="9dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/btnVoda"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Vodovod i Kanalizacija" />

        <Button
            android:id="@+id/btnStruja"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Elektroprivreda" />

        <Button
            android:id="@+id/btnRad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rad" />

        <Button
            android:id="@+id/btnSarajevoStan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sarajevo Stan" />

        <Button
            android:id="@+id/btnTelemach"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Telemach" />

        <Button
            android:id="@+id/btnToplane"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Toplane" />

        <Button
            android:id="@+id/btnHetig"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hetig" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>